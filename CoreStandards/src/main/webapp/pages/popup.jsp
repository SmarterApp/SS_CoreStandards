
<div id="allPopUps"></div>
<div class="popUpTemplate" style="display: none">
	<div class="overlay" style="display: block"></div>
	<div class="popUpBG"></div>
	<div class="popUp">
		<div class="closePop">
			<span class="popClose icon_sprite icon_close cancelButton"></span>
		</div>
		<div class="results">
			<h2 tabindex="-1" class="messageTitle"></h2>
			<div class="errorBlock" style="diplay: none;">
				<div class="errorMessageDiv">
					<span class="icon_sprite icon_error error errorLogoSpanBlock" aria-hidden="true"></span><span
						class="errorMessageSpanBlock">x</span>
				</div>
			</div>
			<div class="warningBlock" style="diplay: none;">
				<div class="warningMessageDiv">
					<span class="icon_sprite icon_warning warning warningLogoSpanBlock" aria-hidden="true"></span><span
						class="warningMessageSpanBlock">x</span>
				</div>
			</div>
			<div class="alertBlock" style="diplay: none;">
				<div class="alertMessageDiv">
					<span class="icon_sprite icon_flag flag alertLogoSpanBlock" aria-hidden="true"></span><span
						class="alertMessageSpanBlock">x</span>
				</div>
			</div>
			<!-- 
			<div class="center tryAgainBlock" style="diplay: none;">
				<button class="boxBtn tryAgainButton">
					<span class="btnIcon entypo-cw"></span><span
						class="btnText buttonMessage">Try Again</span>
				</button>
			</div>  -->
		</div>
	</div>
</div>
